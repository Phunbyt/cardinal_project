{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "prettier",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react","prettier"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "func-names": "off",
        "class-methods-use-this": "off",
        "no-useless-catch": "warn",
        "global-require": "warn",
        "no-use-before-define": [
            "error",
            {
                "variables": false
            }
        ]
    }
}
